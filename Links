stuff to get on 42

sudo apt-get install cowsay
sudo apt-get install fortune

nano ~/.bashrc

if [ -x /usr/games/cowsay -a -x /usr/games/fortune ]; then
    fortune | cowsay -f $(ls /usr/share/cowsay/cows/ | shuf -n1)
fi

